    <tag>
        <name>column</name>
        <display-name>dhtmlx:column</display-name>
        <description>Grid column item</description>
        <tag-class>com.mylaensys.dhtmlx.tags.ColumnTag</tag-class>
        <bodycontent>JSP</bodycontent>
        <attribute>
            <name>name</name>
            <description>Column name (id)</description>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>header</name>
            <description>Column header</description>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>type</name>
            <description>Sets the column type. Valid types are: 'ro' (read only), 'ed' (simple editor), 'txt' text editor, 'ch' checkbox, 'ra' radio button, 'coro' select box , 'co' combobox , 'img' image, 'dhxCalendar' calendar.</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>align</name>
            <description>Sets column alignment. Possible values are: 'right', 'left', 'center' and 'justify'.</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>valign</name>
            <description>Sets column vertical alignment Possible values are: 'baseline', 'sub', 'super', 'top', 'text-top', 'middle', 'bottom', 'text-bottom'.</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>sort</name>
            <description>Sets column sorting. Possible values are: 'str' (data will be sorted as strings case sensitive), 'int' data will be sorted as numbers, 'date' data will be sorted as a date and 'na' sorting is not available for a column.</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>width</name>
            <description>Sets the initial width of columns in pixels</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>i18n</name>
            <description>True|false if i18n has to be applied</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>

