<taglib>
    <tlib-version>1.0</tlib-version>
    <jsp-version>1.2</jsp-version>
    <short-name>dhtmlx-tag</short-name>
    <uri>http://www.mylaensys.com/dhtmlx</uri>

    <tag>
        <name>body</name>
        <display-name>dhtmlx:body</display-name>
        <description>
            Container tag for DHTMLX components. All component tags must be declared within this tag.
        </description>
        <tag-class>com.mylaensys.dhtmlx.tags.BodyTag</tag-class>
        <bodycontent>JSP</bodycontent>
        <attribute>
            <description>Name of the javascript function used to initialize the declared components</description>
            <name>name</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>skin</name>
            <description>Sets the appearance. Predefined skins are: dhx_skyblue (default), dhx_blue, dhx_black and dhx_web.</description>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>imagePath</name>
            <description>Path to image folder.</description>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>i18n</name>
            <description>Name of the resource bundle properties file</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>

    <tag>
        <name>accordion</name>
        <display-name>dhtmlx:accordion</display-name>
        <description>
            Accordion is a component that provides an easy and effective way of placing different types of content on a single page.
            It's a very flexible and easy-to-use content manager, compatible with other components that can be attached to Accordion without any difficulties (supports only vertical layout).
            Accordion menus are often used as a websiteâ€™s main navigation. In this way, it acts much like Navigation Tabs, as menu items are collapsed when a new panel is clicked. Where the Navigation Tabs are most often used horizontally, Accordion menus are used vertically.
            They can however, function quite well as sub-navigation for a specific section of a website.
        </description>
        <tag-class>com.mylaensys.dhtmlx.tags.AccordionTag</tag-class>
        <bodycontent>JSP</bodycontent>
        <attribute>
            <name>name</name>
            <description>Name of the accordion</description>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>id</name>
            <description>The HTML object of the page to which the accordion is attached</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>mode</name>
            <description>Sets the accordion mode. Valid values are <b>single</b> (default) and <b>multi</b>. By setting Single Mode the user clicks on a collapsed item expands this item, and collapses the one that was expanded before.
            Setting Multi Mode when the end-user clicks on a collapsed item expands this item.</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>openItem</name>
            <description>Initial open item</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>onBeforeActive</name>
            <description>User defined function invoked when the user clicks on an item, but before this item becomes active and open</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>onActive</name>
            <description>User defined function invoked when the user clicks on an item and it becomes active</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>onDock</name>
            <description>User defined function invoked when a window docks into the accordion</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>onUnDock</name>
            <description>User defined function invoked when the accordion cell undocks into a window</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>

    <tag>
        <name>accordioncell</name>
        <display-name>dhtmlx:accordioncell</display-name>
        <description>Represents an accordion cell</description>
        <tag-class>com.mylaensys.dhtmlx.tags.AccordionCellTag</tag-class>
        <bodycontent>JSP</bodycontent>
        <attribute>
            <name>name</name>
            <description>Name of the cell</description>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>id</name>
            <description>Attaches an HTML object to a cell</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>text</name>
            <description>Sets item's header</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Attaches an URL to a cell</description>
            <name>url</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>i18n</name>
            <description>True|false if i18n has to be applied</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>hasRole</name>
            <description>Security constraint based on user role</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>hasPermission</name>
            <description>Security constraint based on user permissions</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>                         
    </tag>


    <tag>
        <name>layout</name>
        <display-name>dhtmlx:layout</display-name>
        <description>
            Layout is a component that provides powerful and efficient solution for building web interfaces, allowing to programmatically
            define interface structure, including layout of elements and their sizes.
            Layout provides configurable skins and more than 20 patterns, margins and sizing options, such as fixed size or autosize,
            some actions and effects.
        </description>
        <tag-class>com.mylaensys.dhtmlx.tags.LayoutTag</tag-class>
        <bodycontent>JSP</bodycontent>
        <attribute>
            <name>name</name>
            <description>Name of the layout</description>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>pattern</name>
            <description>The name of a layout's pattern.</description>
            <required>true</required>
            <required>false</required>
        </attribute>
        <attribute>
            <name>id</name>
            <description>The HTML object of the page to which the layout is attached</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <!-- Events -->
        <attribute>
            <name>onDblClick</name>
            <description>User defined function invoked when an item's header is double-clicked</description>
            <required>false</required>
        </attribute>
        <attribute>
            <name>onExpand</name>
            <description>User defined function invoked when an item is expanded</description>
            <required>false</required>
        </attribute>
        <attribute>
            <name>onCollapse</name>
            <description>User defined function invoked when an item is collapsed</description>
            <required>false</required>
        </attribute>
        <attribute>
            <name>onResizeFinish</name>
            <description>User defined function invoked when resizing is finished</description>
            <required>false</required>
        </attribute>
        <attribute>
            <name>onPanelResizeFinish</name>
            <description>User defined function invoked when resizing of a panel is finished</description>
            <required>false</required>
        </attribute>
        <attribute>
            <name>onContentLoaded</name>
            <description>User defined function invoked when the content is loaded</description>
            <required>false</required>
        </attribute>
        <attribute>
            <name>onDock</name>
            <description>User defined function invoked when a window docks into the layout</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>onUnDock</name>
            <description>User defined function invoked when the layout cell undocks into a window</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>

